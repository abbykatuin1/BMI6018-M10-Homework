    "import pandas as pd\n",
    "\n",
    "# Load your flight data\n",
    "df = pd.read_csv(\"flights.csv\")\n",
    "\n",
    "# 1️⃣ Create a 'date' column in the format YYYY/M/D\n",
    "df[\"date\"] = pd.to_datetime(df[[\"year\", \"month\", \"day\"]])\n",
    "\n",
    "# If on Mac/Linux use %-m / %-d, on Windows use %#m / %#d\n",
    "df[\"date\"] = df[\"date\"].dt.strftime(\"%Y/%-m/%-d\")  # Mac/Linux\n",
    "# df[\"date\"] = df[\"date\"].dt.strftime(\"%Y/%#m/%#d\")  # Uncomment for Windows\n",
    "\n",
    "# 2️⃣ Group by date and calculate the average departure delay\n",
    "avg_delay_by_date = df.groupby(\"date\")[\"dep_delay\"].mean()\n",
    "\n",
    "# 3️⃣ Find the date with the largest average departure delay\n",
    "worst_date = avg_delay_by_date.idxmax()   # gives the date\n",
    "worst_avg_delay = avg_delay_by_date.max() # gives the float value\n",
    "\n",
    "# 4️⃣ Create a pandas slice for that date\n",
    "worst_day_slice = df[df[\"date\"] == worst_date]\n",
    "\n",
    "# 5️⃣ Display results\n",
    "print(\"Date with largest average departure delay:\", worst_date)\n",
    "print(\"Average departure delay:\", worst_avg_delay)\n",
    "worst_day_slice.head()\n"
   ]
  },
